{
  "name": "@softwareventures/dictionary",
  "version": "0.0.0-development",
  "description": "Pure functional dictionary manipulation and traversal",
  "keywords": [
    "dictionary",
    "functional",
    "lambda",
    "object",
    "typescript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/dictionary",
  "bugs": "https://github.com/softwareventures/dictionary",
  "repository": "github:softwareventures/dictionary",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "dictionary-types": "^1.2.0 || ^2.0.0",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "4.3.5",
    "@softwareventures/prettier-config": "3.0.0",
    "@softwareventures/semantic-release-config": "2.0.2",
    "@softwareventures/tsconfig": "5.1.1",
    "ava": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.18.0",
    "prettier": "2.7.1",
    "semantic-release": "17.4.7",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures",
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
